{
    "name": "froxel",
    "version": "2.2.0",
    "description": "Froxel Game Framework",
    "type": "module",
    "main": "./dist/froxel.m.js",
    "module": "./dist/froxel.m.js",
    "browser": "./dist/froxel.js",
    "types": "./dist/froxel.d.ts",
    "bin": {
        "froxel": "bin/cli.js"
    },
    "scripts": {
        "test": "echo Ok",
        "requirements": "pnpm global add --ignore-optional parcel",
        "deploy": "pnpm build:all && pnpm tsdoc && node ./scripts/deploy.js",
        "commit": "node ./scripts/commit.js",
        "tsdoc": "node ./scripts/tsdoc.js ./src/main.js ./dist/froxel.d.ts",
        "build:all": "pnpm build:module && pnpm build:browser",
        "build:browser": "parcel build --no-cache --target browser",
        "build:module": "parcel build --no-cache --target module",
        "build": "pnpm build:browser"
    },
    "browserslist": "since 2020-01",
    "targets": {
        "module": {
            "source": "./src/main.js",
            "optimize": true
        },
        "browser": {
            "source": "./src/main.js",
            "optimize": true,
            "includeNodeModules": true
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/rsthn/froxel.git"
    },
    "author": "RedStar Technologies",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/rsthn/froxel/issues"
    },
    "homepage": "https://github.com/rsthn/froxel#readme",
    "dependencies": {
        "filereader": "^0.10.3",
        "fs-extra": "^10.0.0",
        "image-size": "^1.0.0",
        "parcel": "^2.4.1",
        "rinn": "^2.0.39"
    },
    "files": [
        "dist/",
        "bin/",
        "LICENSE",
        "README.md"
    ]
}