{
    "name": "froxel",
    "version": "2.0.49",
    "description": "Froxel Game Engine",
    "type": "module",
    "main": "src/main.js",
    "bin": {
        "froxel": "bin/cli.js"
    },
    "scripts": {
        "test": "echo Ok",
        "requirements": "yarn global add --ignore-optional parcel",
        "deploy": "yarn build && node ./scripts/deploy.js",
        "commit": "yarn build && node ./scripts/commit.js",
        "build": "parcel build --no-cache --no-source-maps --target target_dist src/_export.js"
    },
    "target_dist": "dist/froxel.js",
    "targets": {
        "target_dist": {
            "isLibrary": false,
            "engines": {
                "browsers": "since 2021-01"
            }
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/rsthn/froxel.git"
    },
    "author": "RedStar Technologies",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/rsthn/froxel/issues"
    },
    "homepage": "https://github.com/rsthn/froxel#readme",
    "dependencies": {
        "@rsthn/rin": "^2.0.33",
        "filereader": "^0.10.3",
        "fs-extra": "^10.0.0",
        "image-size": "^1.0.0"
    },
    "files": [
        "src/",
        "dist/",
        "bin/",
        "LICENSE",
        "README.md"
    ]
}