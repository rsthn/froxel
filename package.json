{
    "name": "froxel",
    "version": "2.0.61",
    "description": "Froxel Game Engine",
    "type": "module",
    "main": "src/main.js",
    "types": "./dist/froxel.d.ts",
    "browser": "./dist/froxel.js",
    "bin": {
        "froxel": "bin/cli.js"
    },
    "scripts": {
        "test": "echo Ok",
        "requirements": "yarn global add --ignore-optional parcel",
        "deploy": "yarn build && yarn tsdoc && node ./scripts/deploy.js",
        "commit": "node ./scripts/commit.js",
        "tsdoc": "node ./scripts/tsdoc.js ./src/main.js ./dist/froxel.d.ts",
        "build:browser": "parcel build --no-cache --target browser",
        "build": "yarn build:browser"
    },
    "targets": {
        "browser": {
            "context": "browser",
            "outputFormat": "esmodule",
            "isLibrary": true,
            "scopeHoist": true,
            "optimize": true,
            "sourceMap": true,
            "source": "src/main.js",
            "engines": {
                "browsers": "since 2021-01"
            }
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/rsthn/froxel.git"
    },
    "author": "RedStar Technologies",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/rsthn/froxel/issues"
    },
    "homepage": "https://github.com/rsthn/froxel#readme",
    "dependencies": {
        "@rsthn/rin": "^2.0.33",
        "filereader": "^0.10.3",
        "fs-extra": "^10.0.0",
        "image-size": "^1.0.0"
    },
    "files": [
        "src/",
        "dist/",
        "bin/",
        "LICENSE",
        "README.md"
    ]
}